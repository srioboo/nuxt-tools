{
  "name": "nuxtools",
  "version": "1.0.0",
  "description": "Nuxt utilities apps",
  "author": "salrio",
  "private": true,
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext \".ts,.js,.vue\" --ignore-path .gitignore .",
    "test": "jest",
    "heroku-postbuild": "npm run build"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": "npm run lint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@nuxt/content": "^2.13.4",
    "concurrently": "^5.2.0",
    "vue-quill-editor": "^3.0.6",
    "vuex-module-decorators": "^0.17.0",
    "@nuxt/eslint": "^0.6.1",
    "@vue/test-utils": "^1.0.0-beta.27",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.31.0",
    "husky": "^9.1.6",
    "nuxt": "^3.16.0",
    "jest": "^24.1.0",
    "lint-staged": "^10.0.0",
    "prettier": "^3.3.3",
    "sass": "^1.81.0",
    "stylelint": "^16.10.0",
    "vue-jest": "^4.0.0-0",
   "typescript": "^5.6.3"

  },
  "main": "index.js",
  "repository": "https://github.com/srioboo/VueTools.git",
  "license": "MIT"
}
